{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "outputDirectory": "delivery-date-estimator/build/client",
  "functions": {
    "api/index.mjs": {
      "includeFiles": "delivery-date-estimator/node_modules/.prisma/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}
